generator client {
  provider = "prisma-client-js"
  output   = "../../../../../../node_modules/.prisma/client"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model temp {
  id         Int       @id @default(autoincrement())
  uuid       String    @unique
  created_at DateTime? @default(now())
  updated_at DateTime? @updatedAt
  email      String    @unique
  password   String
}
